    <script>
        // Search functionality
        document.querySelector('.search-input').addEventListener('input', function(e) {
            const searchTerm = e.target.value.toLowerCase();
            document.querySelectorAll('.video-card').forEach(card => {
                const title = card.querySelector('.card-title').textContent.toLowerCase();
                const channel = card.querySelector('.card-channel').textContent.toLowerCase();
                
                if (title.includes(searchTerm) || channel.includes(searchTerm)) {
                    card.style.display = 'block';
                } else {
                    card.style.display = 'none';
                }
            });
        });

        // Fix for video autoplay issue
        const videoModal = document.getElementById('videoModal1');
        const videoFrame = document.getElementById('videoFrame');
        
        // When a video card is clicked, set the correct YouTube video URL
        document.querySelectorAll('.video-card').forEach(card => {
            card.addEventListener('click', function() {
                const videoId = this.getAttribute('data-video-id');
                // Note: removed autoplay=1 from the URL
                videoFrame.src = `https://www.youtube.com/embed/${videoId}`;
            });
        });
        
        // When modal is hidden, clear the iframe src to stop video
        videoModal.addEventListener('hidden.bs.modal', function () {
            videoFrame.src = '';
        });
    </script>
